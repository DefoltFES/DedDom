<Application x:Class="DedDom.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DedDom"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="FirstColor" Color="#4BB9F8"></SolidColorBrush>
        <SolidColorBrush x:Key="SecondColor" Color="#212121" ></SolidColorBrush>
        <SolidColorBrush x:Key="ErrorColor" Color="#FF0000" Opacity="0.6"></SolidColorBrush>
        <FontFamily x:Key="Bold">Fonts/#Roboto Bold</FontFamily>
        <FontFamily x:Key="Medium">Fonts/#Roboto Medium</FontFamily>
        <FontFamily x:Key="Regular">Fonts/#Roboto Regular</FontFamily>
    </Application.Resources>
</Application>
